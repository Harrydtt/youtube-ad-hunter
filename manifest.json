{
  "manifest_version": 3,
  "name": "YouTube Ad Hunter v45",
  "version": "45.4",
  "description": "Copy of Adblock for Youtube - Full Ad Blocking",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "inject.js"
      ],
      "all_frames": true,
      "run_at": "document_start"
    },
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "manager.js",
        "sniper.js",
        "cleaner.js"
      ],
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "tabs",
    "storage",
    "unlimitedStorage",
    "scripting",
    "alarms",
    "declarativeNetRequest",
    "webNavigation",
    "webRequest"
  ]
}